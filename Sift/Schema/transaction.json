{
  "$schema": "http://json-schema.org/draft-04/schema",
  "title": "Transaction",
  "type": "object",
  "required": [ "$type" ],
  "properties": {
    "$type": {
      "type": "string",
      "default": "$transaction"
    },
    "$user_id": {
      "type": [ "string", "null" ]
    },
    "$session_id": {
      "type": [ "string", "null" ]
    },
    "$user_email": {
      "type": [ "string", "null" ]
    },
    "$transaction_type": {
      "type": [ "string", "null" ]
    },
    "$transaction_status": {
      "type": [ "string", "null" ]
    },
    "$amount": {
      "type": ["integer", "null"]
    },
    "$currency_code": {
      "type": [ "string", "null" ]
    },
    "$order_id": {
      "type": [ "string", "null" ]
    },
    "$transaction_id": {
      "type": [ "string", "null" ]
    },
    "$billing_address": {
      "oneOf": [
        { "$ref": "ComplexTypes/address.json" },
        { "type": "null" }
      ]
    },
    "$payment_method": {
      "oneOf": [
        { "$ref": "ComplexTypes/payment_method.json" },
        { "type": "null" }
      ]
    },
    "$shipping_address": {
      "oneOf": [
        { "$ref": "ComplexTypes/address.json" },
        { "type": "null" }
      ]
    },
    "$seller_user_id": {
      "type": [ "string", "null" ]
    },
    "$transfer_recipient_user_id": {
      "type": [ "string", "null" ]
    },
    "$app": {
      "oneOf": [
        { "$ref": "ComplexTypes/app.json" },
        { "type": "null" }
      ]
    },
    "$browser": {
      "oneOf": [
        { "$ref": "ComplexTypes/browser.json" },
        { "type": "null" }
      ]
    }
  }
}
